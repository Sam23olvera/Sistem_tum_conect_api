@{
    ViewData["Title"] = "Listar";
    Layout = "~/Views/Shared/_Layout_ViajesSepomex.cshtml";
    var usuarioModel = ViewData["UsuarioModel"] as UsuarioModel;
}
<div class="container">
    <br />
    <div class="row text-center">
        <h1>Viajes Sepomex</h1>
        <input type="hidden" value="@TempData["Mensaje"]" id="mensaje" />
        <input type="hidden" value="@TempData["guardado"]" id="guarda" />
    </div>
    <div class="container">
        <button class="btn btn-primary" onclick="abrirVentana()">Crear ventana</button>
        <button class="btn btn-primary" onclick="modificarVentana()">Cambiar tamaño de la ventana</button>
        <button class="btn btn-primary" onclick="imprimirPagina()">Imprimir la página actual</button>
        <button class="btn btn-primary" onclick="mostrarDatosEnVentana()">Mostrar datos</button>
        <button class="btn btn-primary" onclick="modificarTextoVentanaSegunSelectorID()">Modificar datos en ventana</button>
        <button class="btn btn-primary" onclick="cerrarVentana()">Cerrar ventana</button>
        <input type="text" value="@ViewData["TextoPrueba"]" id="Texto" name="Texto" />
        <p id="aviso"></p>
    </div>
</div>
